pipeline {
    agent any

    environment{
        REPO_URL = 'git@github.com:nishantindorkar/express.git'
    }

    stages {
        stage('Clean Workspace'){
            steps {
                cleanWs()
            }
        }
        stage('Checkout from Git') {
            steps {
                git branch: 'master', url: "${REPO_URL}"
            }
        }        
    }
}